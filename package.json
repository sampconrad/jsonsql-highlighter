{
  "name": "jsonsql-highlighter",
  "displayName": "SQL Highlighter for JSON",
  "description": "Enhanced syntax highlighting for SQL queries embedded in JSON files with distinct visual elements for strings, variables, and SQL keywords",
  "version": "1.0.8",
  "author": "sampconrad <https://github.com/sampconrad>",
  "homepage": "https://github.com/sampconrad",
  "publisher": "sampconrad",
  "repository": {
    "type": "git",
    "url": "https://github.com/sampconrad/jsonsql-highlighter.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "sql-in-json",
        "aliases": ["SQL in JSON", "sql-json"],
        "extensions": [".json"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sql-in-json",
        "scopeName": "source.sql-in-json",
        "path": "./syntaxes/sql-in-json.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4",
    "@vscode/vsce": "^2.19.0"
  }
}
